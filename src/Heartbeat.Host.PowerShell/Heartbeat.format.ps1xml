        <View>
            <Name>StringDuplicate-UserFriendly</Name>
            <ViewSelectedBy>
                <TypeName>Heartbeat.Runtime.Models.StringDuplicate</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>Instance Count</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Short Value</Label>
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>InstanceCount</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>ShortValue</PropertyName>
                            </TableColumnItem>
