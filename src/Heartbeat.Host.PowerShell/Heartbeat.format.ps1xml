<?xml version="1.0" encoding="utf-8"?>
<Configuration>
    <ViewDefinitions>
        <View>
            <Name>ClrSegment-UserFriendly</Name>
            <ViewSelectedBy>
                <TypeName>Microsoft.Diagnostics.Runtime.ClrSegment</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>Is Large</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Gen 0 Size</Label>
                        <Alignment>right</Alignment>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Gen 1 Size</Label>
                        <Alignment>right</Alignment>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Gen 2 Size</Label>
                        <Alignment>right</Alignment>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Size</Label>
                        <Alignment>right</Alignment>
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>IsLarge</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>Gen0Size</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>Gen1Size</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>Gen2Size</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>Size</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>

